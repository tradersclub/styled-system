{
  "name": "@tradersclub/styled-system",
  "version": "6.0.0",
  "license": "MIT",
  "scripts": {
    "build:cjs": "ttsc -b tsconfig.build.cjs.json",
    "build:esm": "ttsc -b tsconfig.build.esm.json",
    "test": "jest",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./**/*.{ts,js,jsx,tsx}": [
      "yarn eslint --fix",
      "yarn prettier --write",
      "yarn ts-prune --error"
    ]
  },
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "source": "src/index.ts",
  "sideEffects": false,
  "devDependencies": {
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@types/styled-components": "5.1.25",
    "csstype": "3.0.11",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "husky": "7.0.4",
    "jest": "28.0.3",
    "jest-emotion": "11.0.0",
    "jest-styled-components": "7.0.8",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "react": "18.1.0",
    "react-test-renderer": "18.1.0",
    "styled-components": "5.3.5",
    "ts-prune": "0.10.3",
    "ttypescript": "1.5.13",
    "typescript": "4.6.4",
    "typescript-transform-paths": "3.3.1"
  },
  "dependencies": {
    "object-assign": "4.1.1"
  },
  "peerDependencies": {
    "styled-components": ">=5"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tradersclub/styled-system.git"
  }
}
